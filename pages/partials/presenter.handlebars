<div class="presenter">
  {{#if abstract}}
    <p>{{ abstract }}</p>
    <br><br>
  {{/if}}
  {{#if title}}
  <h4>{{ name }}, {{ title }} @ <a href="{{ web }}" class ="link">{{ organisation }}</a><a href="{{ linkedin }}" class ="link"><img src="{{ image }}" alt="{{ name }}" class="presenter-image"></a></h4>
  {{/if}}
  {{#if twitter}}
  <a href="https://twitter.com/{{ twitter }}" class="twitter"><img src="images/twitter.svg" alt="Twitter" style="width:21px"></a>
  {{/if}}
  {{#if github}}
  <a href="https://github.com/{{ github }}" class="github"><img src="images/github.svg" alt="Github"></a>
  {{/if}}

  <div class="bio clearfix">
    <em><p>{{ blurb }}</p></em>
    
  </div>
</div>